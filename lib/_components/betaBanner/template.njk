<div class="app-beta" role="complementary">
    {% if params.fullWidth %}
    <div class="app-beta__container app-width-container">
    {% else %}
    <div class="nhsuk-width-container app-beta__container app-width-container">
    {% endif %}
        <p class="app-beta__content">
        <strong class="nhsuk-tag app-beta__tag">
            Beta
        </strong>
        <span class="app-beta__text">
        {% if params.bannerMsg %}
            {{ params.bannerMsg }}
        {% else %}
            'Beta' means that we think it's good enough to share but we're still testing it.
        {% endif %}
        </span>
        </p>
    </div>
</div>